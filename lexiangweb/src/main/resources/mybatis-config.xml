<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <environments default="mysql">
        <environment id="mysql">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="com.mysql.jdbc.Driver"/>
                <property name="url" value="jdbc:mysql://localhose:3306/lexiang?useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=false"/>
                <property name="username" value="root"/>
                <property name="password" value="datapass"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper class="com.starlight.bi.datalineage.mapper.FieldDictMapper"/>
        <mapper class="com.starlight.bi.datalineage.mapper.TableDictMapper"/>
        <mapper class="com.starlight.bi.datalineage.mapper.JobDictMapper"/>
        <mapper class="com.starlight.bi.datalineage.mapper.TableLineageMapper"/>
        <mapper class="com.starlight.bi.datalineage.mapper.FieldLineageMapper"/>
        <mapper class="com.starlight.bi.datalineage.mapper.JobLineageMapper"/>
        <mapper class="com.starlight.bi.datalineage.mapper.JobTableRefMapper"/>
    </mappers>
</configuration>